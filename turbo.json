{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**/*"]
    },
    "@fireworks/types#build": {
      "outputs": ["dist/**"],
      "cache": false
    },
    "@fireworks/element-types#build": {
      "dependsOn": ["@fireworks/types#build"],
      "outputs": ["dist/**"]
    },
    "@fireworks/core#build": {
      "dependsOn": ["@fireworks/types#build", "@fireworks/element-types#build"],
      "outputs": ["dist/**"]
    },
    "@fireworks/parser#build": {
      "dependsOn": ["@fireworks/types#build", "@fireworks/element-types#build", "@fireworks/core#build"],
      "outputs": ["dist/**"]
    },
    "@fireworks/examples#build": {
      "dependsOn": [
        "@fireworks/types#build",
        "@fireworks/element-types#build",
        "@fireworks/core#build",
        "@fireworks/parser#build"
      ],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
