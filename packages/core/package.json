    "ink": "^4.4.1",
    "openai": "^4.82.0",
    "react": "^18.2.0",
    "uuid": "^9.0.1",
    "zod": "^3.24.1",
    "zod-validation-error": "^2.1.0"
  },
  "devDependencies": {
    "@fireworks/tsconfig": "workspace:*",
    "@types/node": "^20.0.0",
