{
  "name": "@workflow/core",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./examples/*": {
      "import": "./dist/__examples__/*.js",
      "types": "./dist/__examples__/*.d.ts"
    },
    "./jsx-runtime": {
      "import": {
        "types": "./dist/jsx-runtime.d.ts",
        "default": "./dist/jsx-runtime.js"
      }
    }
  },
  "scripts": {
    "build": "hohoro",
    "dev": "node --watch-path=./src dev.mjs",
    "run": "bun run src/cli/index.ts",
    "lint": "biome check .",
    "test": "bun test",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@fireworksai/sdk": "^0.24.7",
    "@mastra/core": "0.2.0-alpha.92",
    "@nyariv/sandboxjs": "^0.8.23",
    "@stdlib/nlp-sentencize": "^0.2.2",
    "@unkey/api": "^0.33.1",
    "ai": "^4.1.17",
    "hohoro": "^0.3.0",
    "openai": "^4.82.0",
    "react": "^19.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.14",
    "@types/node": "^20.0.0",
    "@workflow/tsconfig": "workspace:*",
    "cli-highlight": "^2.1.11",
    "cli-spinners": "^3.2.0",
    "ink": "^5.1.0",
    "typescript": "^5.0.0"
  }
}
