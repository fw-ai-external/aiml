{
  "name": "@fireworks/core",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./runtime/StepValue": {
      "import": "./dist/runtime/StepValue.js",
      "types": "./dist/runtime/StepValue.d.ts"
