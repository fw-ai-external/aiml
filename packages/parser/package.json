{
  "name": "@aiml/parser",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "test": "bun test ./src/**/*.test.ts ./src/*.test.ts"
  },
  "dependencies": {
    "@aiml/shared": "workspace:*",
    "@mdx-js/typescript-plugin": "^0.0.8",
    "acorn": "^8.14.1",
    "ajv": "^8.17.1",
    "arktype": "^2.1.0",
    "dt-python-parser": "^0.9.0",
    "js-sourcemap": "^0.0.6",
    "js-yaml": "^4.1.0",
    "magic-string": "^0.30.17",
    "mdast": "^3.0.0",
    "ohm-js": "^17.1.0",
    "remark-mdx-frontmatter": "^5.0.0",
    "ts-morph": "^21.0.1",
    "unified-aiml": "workspace:*",
    "uuid": "^11.1.0",
    "xml-formatter": "^3.6.2",
    "xml-js": "^1.6.11",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@aiml/tsconfig": "workspace:*",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.7",
    "@types/uuid": "^10.0.0",
    "@types/xml-formatter": "^2.1.1",
    "typescript": "^5.3.3"
  }
}
