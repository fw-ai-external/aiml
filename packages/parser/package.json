{
  "name": "@fireworks/parser",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rm -rf dist tsconfig.tsbuildinfo && tsc --build",
    "dev": "DEBUG=* & bun run dev.ts --watch-dir ./src & tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "bun test ./src/**/*.test.ts",
    "test:e2e": "bun test e2e/"
  },
  "dependencies": {
    "@fireworks/core": "workspace:*",
    "@fireworks/element-config": "workspace:*",
    "@fireworks/types": "workspace:*",
    "@mdx-js/typescript-plugin": "^0.0.8",
    "ajv": "^8.17.1",
    "arktype": "^2.1.0",
    "hohoro": "^0.3.0",
    "js-sourcemap": "^0.0.6",
    "magic-string": "^0.30.17",
    "mdast": "^3.0.0",
    "remark-mdx-frontmatter": "^5.0.0",
    "ts-morph": "^21.0.1",
    "uuid": "^9.0.1",
    "xml-formatter": "^3.6.2",
    "xml-js": "^1.6.11",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@fireworks/tsconfig": "workspace:*",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.7",
    "@types/node": "^20.11.5",
    "@types/uuid": "^9.0.8",
    "@types/xml-formatter": "^2.1.1",
    "typescript": "^5.3.3"
  }
}
