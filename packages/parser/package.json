    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "xml-js": "^1.6.11",
    "xml-formatter": "^3.6.2"
  },
  "devDependencies": {
    "@fireworks/tsconfig": "workspace:*",
    "@types/node": "^20.11.5",
    "@types/uuid": "^9.0.8",
    "@types/xml-formatter": "^2.1.1",
    "typescript": "^5.3.3"
  }
