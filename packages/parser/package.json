{
  "name": "@fireworks/parser",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  },
  "scripts": {
    "build": "tsup ./src/index.ts",
    "dev": "node --watch-path=./src dev.mjs",
    "lint": "eslint src/",
    "test": "bun test",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@fireworks/core": "workspace:*",
    "@fireworks/element-types": "workspace:*",
    "ts-morph": "^21.0.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@fireworks/tsconfig": "workspace:*",
    "@types/node": "^20.11.5",
    "@types/uuid": "^9.0.8",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
