{
  "name": "@fireworks/language-service",
  "version": "0.6.2",
  "type": "module",
  "description": "AIML support for Volar",
  "homepage": "https://fireworks.com",
  "bugs": "https://github.com/fwai/aiml/issues",
  "funding": "https://opencollective.com/unified",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "keywords": [
    "IntelliSense",
    "aiml",
    "llm",
    "ai",
    "language-service",
    "vscode",
    "fireworks",
    "prompt",
    "llm",
    "llm-prompt",
    "llm-prompt-engine",
    "llm-prompt-engineer"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
