  "devDependencies": {
    "@types/node": "^22.0.0",
    "eslint": "^8.0.0",
    "rimraf": "^5.0.0",
    "remark-frontmatter": "^5.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "sideEffects": false
}
