    "mdast-util-mdxjs-esm": "^2.0.0",
    "mdast-util-to-markdown": "^2.0.0",
    "remark-mdx": "^3.0.0",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.0",
    "unist-util-lsp": "^2.0.0",
    "unist-util-visit-parents": "^6.0.0",
    "vfile-message": "^4.0.0",
    "vscode-uri": "^3.0.0"
